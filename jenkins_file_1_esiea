pipeline {
    agent any
    
    stages{
        
        stage('HELLO')
        {
            steps{
                echo'Hello World'
            }
        }
        stage('BUILD'){
            steps{
                echo'Developpemnt'
            }
        }
        
        stage('Test'){
            steps{
                echo 'Testing'
            }
        }
        
        stage('Deploy'){
            steps{
                echo 'Mise en Production'
            }
        }
    }
}
